.buildList_box {
  margin-top: 30rpx;
  padding-bottom: 30rpx;
  /* height: 600rpx; */
}

.buildList_box .ul {
  white-space: nowrap;
}

.ul {
  display: inline-block;
}

.ul .li {
  display: inline-block;
  vertical-align: top;
  width: 600rpx;
  height: 550rpx;
  box-shadow: #ccc 0px 0px 20rpx;  
  /* margin-top:30rpx; */
  margin:15rpx 20rpx 60rpx 10rpx;
}

.ul .li:last-child {
  margin-right: 0;
}

.li .images {
  width: 100%;
  height: 400rpx;
  border-radius: 5px;
 
}
.li .rentBuidinfo{
  width:100%;
  height: 150rpx;
}
.li .rentBuidinfo .build_name{
  width: 95%;
  padding-top: 20rpx;
  color: #333333;
  margin-left: 5%;
  font-size: 32rpx;
}
.li .rentBuidinfo .price{
  width: 28%;
  float: left;
  margin-left: 5%;
  color: #F43333;
  padding-top: 5rpx;
}
.li .rentBuidinfo .tags{
  width:65%;
  float: right; 
  padding-top: 5rpx;
}
.li .rentBuidinfo .tags .tagsText{
  background: #F4F8FB;
  font-size: 28rpx;
  height: 50rpx;
  display: inline-block;
  line-height: 50rpx;
  border-radius: 5rpx;
  padding-left:8rpx;
  padding-right: 8rpx;
  margin-left:10rpx;
}
.li .buildInfo{
  width:95%;
  height: 150rpx;
  position: relative;
  margin-left:5%;
}
.li .buildInfo .buildAddress{
  position: absolute;
  font-size: 24rpx;
  right:10rpx;
  top: 20rpx;
  width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.li .buildInfo .build_name {
  font-size: 32rpx;
  padding-top: 20rpx;
}

.li .buildInfo .price {
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color:#ccc;
}

.li .buildInfo .price .number {
  font-size: 30rpx;
  color: #F43333;
}

