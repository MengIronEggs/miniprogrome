.rent_list {
  padding-top: 36rpx;
}

.house_list {
  margin-top: 40rpx;
}

.house_list .left_img {
  width: 70%;
  height: 320rpx;
  float: left;
  position: relative;
}

.left_img .images {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.left_img .prices {
  position: absolute;
  left: 20rpx;
  top: 10%;
  width: 180rpx;
  height: 60rpx;
  background: rgba(0, 0, 0, 0.5);
  line-height: 50rpx;
  text-align: center;
  color: white;
  box-sizing: border-box;
  padding: 7rpx 5rpx 10rpx 25rpx;
  font-size: 28rpx;
}

.left_img  .prices_number {
  font-size: 36rpx;
  line-height: 50rpx;
}

.house_list {
  /* border-bottom: 1px solid #eee; */
  box-shadow: #ccc 0px 0px 30rpx;  
}

.house_list .right_box {
  width: 28%;
  height: 320rpx;
}

.house_list .right_box .right_img {
  width: 100%;
  height: 47%;
  border-radius: 5rpx;
}

.house_list .right_img .images {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.house_list  .right_box .right_img:nth-child(2) {
  margin-top: 8%;
}

.house_list .build_info {
  width: 100%;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-bottom: 36rpx;
  height: 130rpx;
}
.build_info .list-right-price{
  width:40%;
  height: 45rpx;
   font-size: 32rpx;
  color: #F43333;
  float: right;
  text-align: right;
  padding-right:20rpx;
}
.build_info .list-bottom-left {
  width: 50%;
  height: 45rpx;
  float: left;
  font-size: 32rpx;
  color: #000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
  text-indent: 10rpx;
  margin-left:5%;
}

.list-bottom-right {
  width: 50%;
  height: 40rpx;
  float: left;
  font-size: 28rpx;
  color: #666;
  text-align: left;
  margin-left: 6%;
}

.bottom_btn {
  position: fixed;
  border-top: 1px solid #ccc;
  background: #f7f7fa;
  left: 0;
  bottom: 0;
  height: 100rpx;
  width: 100%;
}

.bottom_btn .btn {
  width: 25%;
  height: 110rpx;
  float: left;
}

.btn .btn_img {
  width: 26px;
  height: 26px;
  margin: 10rpx auto 4rpx;
  position: relative;
}

.btn .btn_img .bgi {
  width: 92%;
  height: 92%;
}

.btn .btn_name {
  font-size: 21rpx;
  text-align: center;
  color: #999;
}

.btn:nth-child(1) .btn_name {
  color: black;
}
