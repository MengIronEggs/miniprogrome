/**index.wxss**/

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.index_page {
  padding: 5px 0 100rpx;
}

.cards {
  padding: 0 30rpx;
}

.bgi {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.swipers {
  position: relative;
}

/* 三个点 */

.bigSlide {
  width: auto;
  margin: 0 auto;
  height: 50rpx;
  text-align: center;
}

.bigSlide .smallSlidePage {
  width: 30rpx;
  height: 10rpx;
  display: inline-block;
  background: #CCCCCC;
  margin-top: 25rpx;
  margin-left: 10rpx;
  border-radius: 5rpx;
}
.bigSlide .slideActive{
  background: #F43333;
}

.section {
  width: 94%;
  margin-left: 3%;
  margin-top: 10rpx;
  /* background-color:rgba(255,255,255,0.6); */
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  border-radius: 5px;
  box-shadow: #ccc 0px 0px 30rpx;
  color: #ccc;
}

.section .left_box {
  float: left;
  width: 80rpx;
  height: 80rpx;
  position: relative;
}

.section .left_box .bgi {
  width: 50%;
  height: 50%;
}

.section .middle_box {
  /* position: absolute; */
  width: 100%;
  left: 0;
  top: 0;
  padding: 0 80rpx;
  box-sizing: border-box;
  color: #ccc;
}

.section .right_box {
  float: right;
  width: 80rpx;
  height: 80rpx;
  position: relative;
  /* background: #000; */
}

.section .right_box .adress {
  width: 100%;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
  border-left: 1px solid #fff;
  height: 40rpx;
}

.section .right_box .adress .bgi {
  width: 40%;
  height: 90%;
}

.swiper_box {
  height: 400rpx;
  border-radius: 5px;
  overflow: hidden;
}

.slide-image {
  width: 100%;
  height: 400rpx;
}

.bars {
  width: 100%;
  padding: 60rpx 0;
}

.bars .btn {
  width: 20%;
  float: left;
  height: 160rpx;
  text-align: center;
}

.bars .btn-img {
  vertical-align: top;
  width: 90rpx;
  height: 90rpx;
}

.bars .btn_info {
  width: 100%;
  margin-top: 8rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
}

.lines {
  border-bottom: 20rpx solid #eee;
}

.titles {
  margin-top: 36rpx;
  font-size: 36rpx;
}

.postImg {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  height: 186rpx;
}

.postImg .images {
  width: 100%;
  height: 100%;
}

