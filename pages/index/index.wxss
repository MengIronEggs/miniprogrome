.info_box {
  margin-top: 30rpx;
  padding-bottom: 20rpx;
}

.left_box {
  float: left;
}

.info_box .ac_imgbox {
  width: 320rpx;
  height: 240rpx;
}

.ac_imgbox .ac_img {
  width: 100%;
  height: 100%;
}

.right_box {
  float: right;
}

.ac_infobox {
  width: 50%;
  padding-bottom: 20rpx;
}

.ac_infobox .titles3 {
  font-size: 34rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ac_infobox .tags {
  width: 100rpx;
  height: 36rpx;
  background: #e34b3e;
  text-align: center;
  line-height: 36rpx;
  font-size: 22rpx;
  color: white;
  padding: 0 8rpx;
  margin: 24rpx 0;
}

.ac_infobox  .ac_infos {
  width: 100%;
