/**index.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
/* @font-face{
  font-family:font01;
  src : url('/fonts/BEBASNEUE.otf');
} */
/* .font01{
  font-family: font01
} */
.index_page{
  padding: 5px 0 100rpx;
}
.cards{
  padding: 0 30rpx;
}
.bgi{
  position: absolute;
  left: 50%;
  top:50%;
  transform: translate(-50% ,-50%)
}
.swipers{
  position: relative;

}
.section{
  position: absolute;
  top:20rpx;
  width: 92%;
  left: 4%;
  background-color:rgba(255,255,255,0.6);
   height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  border-radius: 5px;
}
.section .left_box{
  float: left;
  width: 80rpx;
  height: 80rpx;
  position: relative;
 
}
.section .left_box .bgi{
  width: 50%;
  height: 50%;
}
.section .middle_box{
  position: absolute;
  width: 100%;
  left: 0;
  top:0;
  padding:0 80rpx ; 
  box-sizing: border-box;
  color: #fff;
}
.section .right_box{
  float: right;
  width: 80rpx;
  height: 80rpx;
  position: relative;
  /* background: #000; */
}
.section .right_box .adress{
  width: 100%;
  position:absolute;
  top:50%;
  left: 0;
  transform: translateY(-50%);
  border-left:1px solid #fff;
  height: 40rpx;
}
.section .right_box .adress .bgi{
  width: 40%;
  height: 90%;
}
.swiper_box{
  height: 400rpx;
  border-radius: 5px;
  overflow: hidden;
}
.slide-image{
  width: 100%;
  height: 400rpx;
}
.bars{
  width:100%;
  padding: 60rpx 0;  
}
.bars .btn {
  width: 20%;
  float: left;
  height: 160rpx;
  text-align: center;
}
.bars .btn-img{
  vertical-align: top;
  width: 90rpx;
  height:90rpx;
}
.bars .btn_info{
  width: 100%;
  margin-top: 8rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
}
.lines{
  border-bottom: 20rpx solid #eee;
}
.titles{
  margin-top: 36rpx;
  font-size: 36rpx;
}
.postImg{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  height: 186rpx; 
}
.postImg .images{
  width: 100%;
  height: 100%;
}
.line2{
  margin-top: 40rpx;
  margin-bottom: 40rpx;

  border-bottom: 2px solid #eee;
}
.titles2 .left_box{
  float: left;
  width: 50%;
  font-size: 34rpx;
}
.titles2  .ac_more{
  float: right;
  width: 18%;
  height: 30rpx;
  position: relative
}
.titles2  .ac_more .bgi{
  width: 80%;
  height: 80%;

}
.info_box{
  margin-top: 30rpx;
  padding-bottom:20rpx; 
 
}
.left_box{
  float: left;
}
.info_box .ac_imgbox{
  width: 320rpx;
  height: 240rpx;
}
.ac_imgbox .ac_img{
  width: 100%;
  height: 100%;
}
.right_box{
  float: right;

}
.ac_infobox{
  width: 50%;
  padding-bottom:20rpx; 
}
.ac_infobox .titles3{
  font-size: 34rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ac_infobox .tags{
    width: 100rpx;
    height: 36rpx;
    background: #E34B3E;
    text-align: center;
    line-height: 36rpx;
    font-size: 22rpx;
    color: white;
    padding: 0 8rpx;
    margin: 24rpx 0;
}
.ac_infobox  .ac_infos{
  width: 100%;
  color: #666666;
  font-size: 28rpx;
  margin-top: 10rpx;
  text-align: left;
  height: 130rpx;

}
.houseCard{
  padding-bottom: 40rpx;
}
.Tips{
    line-height: 40rpm;
    color: #999999;
    font-size: 23rpx;
    text-align: left;
    margin-top: 30rpx;
}
.buildList_box{
  margin-top: 30rpx;
}
.buildList_box .ul{
  white-space: nowrap;
}
.ul{
  display: inline-block;
}
.ul .li{
  display: inline-block;
  vertical-align: top;
  width: 320rpx;
  margin-right:30rpx;
}
.ul .li:last-child{
   margin-right:0;
}
.li .images{
  width: 100%;
  height:240rpx;
  border-radius: 5px;
}
.li .build_name{
  font-size: 32rpx;
}
.li .price{
  font-size: 24rpx;
  overflow: hidden;
}
.li .number{
  font-size: 36rpx;
 
}
.yuekan{
  padding-top: 50rpx;
  padding-bottom: 30rpx;
}
.yuekan_img{
  width:100%;
  height: 150rpx;
}
.yuekan_img .images{
  width: 100%;
  height: 100%;
}
.y_btn_box{
  margin-top: 27rpx;
  height: 130rpx;
}
.y_btn_box .left_box{
  
  width: 48%;
  height: 100%;
}
.y_btn_box  .lines4{
  float: left;
  width: 0;
  margin-top: 30rpx;
  height: 74rpx;
  border-left: 1px solid #ccc; 
  
  /* background: red; */
}
.y_btn_box .right_box{
   width: 48%;
  height: 100%;
}     
.y_btn_box .yuyueImg{
  margin-left: 35rpx;
  width: 30%;
  height: 100%;
  float: left;
  position: relative;
}
.y_btn_box .yuyueImg .bgi{
  width:78%;
  height: 60%;
}
.y_btn_box .describe{
  width: 58%;
  height: 100%;
  float: right;
}
.y_btn_box .describe .texts1{
  margin-top: 21rpx;
  text-align: left;
  font-size: 30rpx;
  color: black;
}
.y_btn_box .describe .texts2{
  text-align: left;
  font-size: 24rpx;
  margin-top: 9rpx;
  color: #666666;
}
.rent_list{
  padding-top:36rpx; 
}
.house_list{
  margin-top: 40rpx;
}
.house_list .left_img {
  width: 68%;
  height: 320rpx;
  float: left;
  position: relative;
}
.left_img .images{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.left_img .prices{
    position: absolute;
    left: 20rpx;
    top: 10%;
    width: 180rpx;
    height: 60rpx;
    background: rgba(0, 0, 0, 0.5);
    line-height: 50rpx;
    text-align: center;
    color: white;
    box-sizing: border-box;
    padding: 7rpx 5rpx 10rpx 25rpx;
    font-size: 28rpx;
}
.left_img  .prices_number{
  font-size: 36rpx;
  line-height: 50rpx;
}
.house_list{
  border-bottom: 1px solid #eee;
}
.house_list .right_box{
  width: 28%;
  height: 320rpx;
}   
.house_list .right_box .right_img{
  width: 100%;
  height: 46%;
  border-radius: 5rpx;
}
.house_list .right_img .images{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.house_list  .right_box .right_img:nth-child(2){
  margin-top: 13%;
}
.house_list .build_info{
  width: 100%;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-bottom: 36rpx;
}
.build_info .list-bottom-left{
    width: 50%;
    height: 45rpx;
    float: left;
    font-size: 32rpx;
    color: #000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
    text-indent: 10rpx;
}
.list-bottom-right{
  width: 50%;
  height: 40rpx;
  float: right;
  font-size: 28rpx;
  color: #666;
  text-align: right;
}
.bottom_btn{
  position: fixed;
  border-top:1px solid #ccc;
  background:#F7F7FA; 
  left: 0;
  bottom: 0;
  height: 100rpx;
  width:100%;
}
.bottom_btn .btn{
  width: 25%;
  height: 110rpx;
  float: left;
}
.btn .btn_img{
  width: 26px;
  height: 26px;
  margin: 10rpx auto 4rpx;
  position: relative;
  
}
.btn .btn_img .bgi{
  width: 92%;
  height:92%;
}
.btn .btn_name{
    font-size: 21rpx;
    text-align: center;
    color:#999999;
}   
.btn:nth-child(1) .btn_name{
  color:black;
}