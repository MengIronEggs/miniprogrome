  text-align: center;
}
.btn_box{
  height: 80rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0; 
}
.btn_box .btn_sty{
  width: 50%;
  height: 100%;
  float: left;
  text-align: center;
  font-size: 34rpx;
  line-height: 80rpx;
  background-color: #ccc;
  color: #fff;
}
.btn_box .btn_sty.red{
  background-color: #d6000f;
  
}