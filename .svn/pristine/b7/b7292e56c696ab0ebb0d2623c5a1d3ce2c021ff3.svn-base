}
.type_info{
  padding-left:186rpx; 
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.number{
  color: #d6000f;
  font-size: 38rpx;
  
}
.danwei{
  color: #d6000f;
  margin-left: 15rpx;
  line-height: 60rpx;
  
}
.houseComput{
  position: absolute;
  top:20rpx;
  right:30rpx;
  width: 140rpx;
  height:140rpx;
}
.btnBg{
  height: 80rpx;
  width: 80rpx;
  margin: 0 auto;
}
.btnBg .somImg{
  width: 100%;
  height:100%;
  
}
