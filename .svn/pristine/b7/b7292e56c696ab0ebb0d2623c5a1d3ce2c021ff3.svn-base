}
.titles{
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;  
  font-size: 30rpx;
  float: left;
  font-weight: 700;
  text-align-last: justify;
}
.maohao{
  float:left;
  width: 30rpx;
  line-height: 60rpx;  
