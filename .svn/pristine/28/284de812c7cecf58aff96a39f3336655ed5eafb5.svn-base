/* pages/personalCenter/personalCenter.wxss */
.personalCenter{
  padding: 0 30rpx;
}
.hidden{display:none;}
.personalCenter .header{
  width: 100%;
  height: 320rpx;
  position: relative;
}
.loginIsRegister{
  font-size: 36rpx;
}
.personalCenter .header .image{
  position: absolute;
  display: inline-block;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.personalCenter .header .imageTwo{
   position: absolute;
   right: 15rpx;
   top: 15rpx;
   width: 40rpx;
   height:40rpx;
   z-index: 1001;
}
.personalCenter .header .header_info{
  display: flex;
  width: 90%;
  height:150rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate3d(-50%,-50%,0);
   z-index: 2;
}
.personalCenter .header .header_info .header_info_left{
    height: 150rpx;
    width: 150rpx;
}
.personalCenter .header .header_info .header_info_left .imageOne{
   display: block;
   width: 100%;
   height: 100%;
}
.personalCenter .header .header_info .header_info_right{
    height: 150rpx;
    margin-left: 20rpx;
}
.personalCenter .header .header_info .header_info_right .header_info_right_height{
  margin-top: 35rpx;
}
.personalCenter .header .header_info .header_info_right .header_info_right_height .header_info_right_top{
  display: flex;
  color:#fff;
  height: 36rpx;
  align-items: center;
  margin-bottom: 15rpx;
  
}
.personalCenter .header .header_info .header_info_right .header_info_right_height .header_info_right_top .oginIsRegister{
  font-size: 36rpx;
  
}
.personalCenter .header .header_info .header_info_right .header_info_right_height .header_info_right_top .SignIn{
  background: #fff;
  font-size: 28rpx;
  color:#e34b3e;
  padding: 0 20rpx;
  border-radius: 30rpx;
  margin-left: 30rpx;
}
.personalCenter .header .header_info .header_info_right .header_info_right_height .header_info_right_bottom{
  font-size: 28rpx;
  color:#fff;
}
/* list */
.personalCenter .list{
  display: flex;
  text-align: center;
  border-bottom: 10rpx solid #eee;
}
.personalCenter .list .list_li{
  flex:1;
  margin: 30rpx 0;
}
.personalCenter .list .list_li .url{
   width: 45rpx;
   height: 45rpx;
}
.personalCenter .list .list_li .name{
  font-size: 24rpx;
}
/* 新房列表 */
.personalCenter .new_list{
  width: 100%;
  display: flex;
  padding-bottom: 30rpx;
  border-bottom:1rpx solid #eee;
}
.personalCenter .new_list .new_list_li{
   flex:1;
  text-align: center;
  background: #eeeeee;
  margin: 0 10rpx;
  padding:20rpx 0;
  border-radius: 10rpx;
}
.personalCenter .new_list .new_list_li .url{
   width: 45rpx;
   height: 45rpx;
}
.personalCenter .new_list .new_list_li .name{
  font-size: 24rpx;
}
.new{
  margin: 20rpx 0;
}
.rent{
  margin: 20rpx 0;
}
.new_img{
  width: 100%;
  height: 160rpx;
}
/* 服务热线 */
.hotline_phone{
    display: flex;
   height: 100rpx; 
   justify-content: space-between;
   align-items: center;
    border-bottom:1rpx solid #eee;
}
.hotline_phone .hotline{
   font-size: 36rpx;
}
.hotline_phone .phone{
   font-size: 28rpx;
}
.personalCenter .Btn_bottom_li .li{
  width: 100%;
  display: flex;
  height: 100rpx;
  border-bottom:1rpx solid #eee;
  justify-content: space-between;
  align-items: center;
}
.personalCenter .Btn_bottom_li .url{
   width: 20rpx;
   height: 45rpx;
   display: block;
}
.personalCenter .Btn_bottom_li .name{
  font-size: 36rpx;
  display: block;
}
.button{
  background:#fff;
  padding:0; 
  box-sizing:border-box;
  font-size:36rpx;
  text-align:left;
  text-decoration:none;
  border-radius:0;
  -webkit-tap-highlight-color:transparent;
  height: 100rpx;
  outline:0 none !important;
  border: 1rpx solid #fff;
  line-height: 100rpx;
  margin: 0;
  color:black;
  display: inherit-block;
    width: 100%;
  position: absolute;
}
button::after {
  border: none
}
.share{
  position: relative;
  height: 100rpx;
  width: 100%;
  border-bottom: 1rpx solid #ddd;
}
.singnSuccess{
  width: 80%;
  height:800rpx;
  background: url("https://img.guoanfamily.com/miniProgram/qdSuccess.png") no-repeat center;
  background-size: 100%;
  position: fixed;
  top: 10%;
  left: 10%;
  z-index: 1151;
}
.market{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
  position: fixed;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  margin: auto;
  z-index: 1002;
}
.qdBtn {
  position: absolute;
  background-size: 50%;
  font-size:40rpx;
  width: 500rpx;
  background: url("https://img.guoanfamily.com/miniProgram/qdlook.png") no-repeat center;
  background-size: 100%;
  color: #fe5137;
  margin-left: 25%;
  width: 50%;
  line-height: 90rpx;
  height: 100rpx;
  bottom: 110rpx;
  border-radius: 150rpx;
  text-align: center;
}

