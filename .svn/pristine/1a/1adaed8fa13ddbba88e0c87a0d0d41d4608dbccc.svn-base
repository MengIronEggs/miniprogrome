/* pages/rent/rentDetails/rentDetails.wxss */
.rentDetails{
  padding: 0 30rpx;


}
.swipers{
  position: relative;

}
.swipers .Danmu{
  position: absolute;
  top:10rpx;
  height: 0;
  left: 0;
  right: 0;
}
.absitemp_img{
  float: left;
  height: 30rpx;
  width: 30rpx;
  margin: 4rpx;
}
.danmu_bg{
  width: auto;
  position: absolute;
  font-size:26rpx;
  padding: 8rpx 20rpx;
  background:  rgba(0,0,0,0.6);
  color: #fff;
  border-radius: 22rpx;
  white-space: nowrap;
}
.headBtn{
  width: 60rpx;
  height:60rpx;
  position: absolute;
  right:20rpx;
  top:20rpx;
}
.headBtn .hertimage{
  width: 100%;
  height:100%;
}
.section{
  position: absolute;
  top:20rpx;
  width: 92%;
  left: 4%;
  background-color:rgba(255,255,255,0.6);
   height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  border-radius: 5px;
}
.section .left_box{
  float: left;
  width: 80rpx;
  height: 80rpx;
  position: relative;
 
}
.section .left_box .bgi{
  width: 50%;
  height: 50%;
}
.section .middle_box{
  position: absolute;
  width: 100%;
  left: 0;
  top:0;
  padding:0 80rpx ; 
  box-sizing: border-box;
  color: #fff;
}
.section .right_box{
  float: right;
  width: 80rpx;
  height: 80rpx;
  position: relative;
  /* background: #000; */
}
.section .right_box .adress{
  width: 100%;
  position:absolute;
  top:50%;
  left: 0;
  transform: translateY(-50%);
  border-left:1px solid #fff;
  height: 40rpx;
}
.section .right_box .adress .bgi{
  width: 40%;
  height: 90%;
}
.swiper_box{
  height: 500rpx;
  border-radius: 5px;
  overflow: hidden;
}
.slide-image{
  width: 100%;
  height:500rpx;
}
