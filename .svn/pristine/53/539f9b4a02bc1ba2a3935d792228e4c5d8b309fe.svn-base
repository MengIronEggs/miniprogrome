.topSearch{
  height: 60rpx;
  padding: 10rpx 0;
  z-index: 2;
  line-height:  60rpx;
  border-top:1px solid #ccc;
  border-bottom:1px solid #ccc; 

}

.seach_btn{
  width: 25%;
  height: 60rpx;
  box-sizing: border-box;
  float: left;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
  border-right:1px solid #ccc;
}
.seach_btn .rc{
  color: #d6000f;
}
.seach_btn .smallimg{
   width: 26rpx;
   height: 16rpx;
   margin-left: 10rpx;
  vertical-align: middle;
  transform: translateY(-20%);
   
}
.seach_btn .smallimg.smallimg2{
  vertical-align: middle
}
.searchList{
  position: fixed;
  bottom: 0;
  top: 84rpx;
  width: 100%;
  z-index: 2;
  background: #fff;
}

.scroll_box{
  position: absolute;
  top: 0;
  bottom: 80rpx;
  width: 100%;

}
.titles{
  margin-top: 30rpx; 
  line-height: 60rpx;
  font-size: 30rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
}
.btn_box{
  height: 80rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}

.btn_box .btn{
  width: 50%;
  float: left;
  color:#fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  background-color: #ccc;
}

.btn_box .btn:nth-child(1){
  background-color: #e34b3e;
}

.searchList .ul{
  position: relative;
  padding: 0 30rpx;
  clear: both;
  overflow:hidden;
}

.searchList .li{
  float: left;
  width: 30%;
  margin-top: 20rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
  margin-left: 3%;
  box-sizing: border-box;
  border:1px solid #ccc;
  height: 60rpx
}
.searchList .li.actived{
   border:1px solid #d6000f;
   color: #d6000f
}
.seach_btn:last-child{
  border: none;
}

.build_List{
  position: fixed;
  bottom: 0;
  left: 0;
  top: 84rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  right: 0;

}
.build_li{
  margin-top: 30rpx;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  padding: 25rpx 0;
  height: 260rpx;
  box-sizing: border-box;
}
.build_li:first-child{
  border-top: none;
}
.build_li::before{
  content: "";
  width: 0;
  height: 0;
  display: none;
  clear: both;
}
.list_img{
  width: 280rpx;
  height: 210rpx;
  float: left;
  border-radius: 10rpx ;
}
.right_box{
 padding-left: 300rpx;
 box-sizing: border-box;
}
.build_name{
  font-size: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.price{
  margin-top: 20rpx;
  font-size: 32rpx;
  color: #666;
}
.average{
  font-size: 28rpx
}
.Searchs{
  height: 100%;
  width: 100%
}
.myslider2_box{
  position: fixed;
  width: 100%;
  top: 500rpx;
  padding: 0 60rpx;
  box-sizing: border-box;
  left: 0;
  z-index: 9999;
}
/* .myslider2_box{
  position: absolute;
  width: 100%;
  padding: 0 30rpx;
} */

.image{
  width: 480rpx;
  height: 480rpx;
  margin: 0 auto;
}
.image>image{
  width: 100%;
  height: 100%;
  display: block;
}
.loading{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.8);
  z-index: 1001;
  overflow: hidden;
}
.loading>image{
  width: 200rpx;
  height: 200rpx;
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate3d(-50%,-50%,0)
}