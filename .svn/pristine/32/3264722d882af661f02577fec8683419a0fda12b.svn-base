/* components/house_list/house_list.wxss */
.ul{
  height: 360rpx;
}
.house_list{
  width: auto;
  display: flex;
  position: relative;
 
  box-sizing: border-box;
  margin-top: 10rpx;
}
.house_list::scrollbar{
  display: none;
}
.house_list .house_list_li{
   width: 320rpx;
   margin-right: 20rpx;
}
.house_list .house_list_li .house_list_li_img{
    width: 320rpx;
    height: 240rpx;
    border-radius:10rpx;
     position: relative;
}
.house_list .house_list_li .house_list_li_img .imgs{
   width: 100%;
   height:100%;
   border-radius:10rpx;
   display: block;
}
.house_list .house_list_li .house_list_li_img .imgOne{
    width:130rpx;
    height: 40rpx;
    position: absolute; 
    bottom: 10rpx; 
    right: 10rpx; 
    z-index: 2;
}
.house_list_li_content{
  margin-top: 20rpx;
  font-size: 32rpx;
  text-indent: 10rpx;
}
.house_list_li_content .one{
  height: 50rpx;
  font-size: 32rpx;
  line-height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

}
.house_list_li_content .two{
  display: flex;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  
}
.house_list_li_content .two .price{
    font-size: 36rpx;
    font-family: font01;
    font-weight: 600;
}
   
.house_list_li_content .two .month{
  font-size: 24rpx;
  font-family: Source Han Sans CN;
}