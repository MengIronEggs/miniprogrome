/* components/new_title/new_tltle.wxss */
.new_title{
  width: 100%;

}
.new_title .new_title_top{
  display: flex;
   align-items:center;
}
.new_title .new_title_top .image{
    width: 32rpx;
    height: 32rpx;
}
.new_title .new_title_top .city{
  font-size: 32rpx;
  margin-left: 15rpx;
}
.new_title .new_title_bottom{
  margin-top: 20rpx;
  display: flex;
  justify-content:space-between;
  align-items:center;
}
.new_title .new_title_bottom .loupan{
  font-size:42rpx;
  margin-left: 47rpx;
}
.new_title .new_title_bottom .price{
  font-size:32rpx;
  margin-right: 30rpx;
  width: 200rpx;
    overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.new_house_lB{
  height: 360rpx
}
.new_house_list{
  margin: 30rpx 0;
  /* width: auto; */
  display: flex;
  position: relative;
  overflow: hidden;
  overflow-x: scroll;
}
.new_house_list .new_house_list_li{
  margin-right: 20rpx;
}
.new_house_list .new_house_list_li .house_list_li_img{
  width: 480rpx;
  height: 320rpx;
  border-radius: 50rpx;
}
.new_house_list .new_house_list_li .image{
  display: inline-block;
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
